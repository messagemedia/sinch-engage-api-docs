{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Sinch Engage API Documentation",
  "colors": {
    "primary": "#007171",
    "light": "#059688",
    "dark": "#ffcf6e"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "API Reference",
        "openapi": "/openapi.yaml",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/index"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://mintlify.com/community",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/messagemedia.png",
    "dark": "/logo/messagemedia.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://support.messagemedia.com"
      },
      {
        "label": "Status",
        "href": "https://status.messagemedia.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://hub.messagemedia.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://twitter.com/messagemedia",
      "github": "https://github.com/messagemedia",
      "linkedin": "https://www.linkedin.com/company/messagemedia"
    }
  }
}
